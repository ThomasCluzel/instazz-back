{
  "name": "instazz-back",
  "version": "0.1.0",
  "description": "Backend of the InstaZZ app of the course ZZ3-javascript-ecosystems",
  "main": "app.js",
  "scripts": {
    "test": "set NODE_ENV=test&& mocha --require @babel/register --timeout 10000",
    "start": "set NODE_ENV=dev&& nodemon app.js --exec babel-node --",
    "heroku-postbuild": "mkdir src $PWD/uploads && cp -r app.js libs posts users src && ./node_modules/.bin/babel src -d build && rm -rf src"
  },
  "keywords": [
    "test",
    "Express"
  ],
  "author": "Thomas Cluzel et Bruno Jousse",
  "license": "ISC",
  "dependencies": {
    "chai-datetime": "^1.5.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.8.1",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/helper-plugin-utils": "^7.0.0",
    "@babel/node": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/runtime": "^7.8.7",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^7.1.0",
    "nodemon": "^2.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
